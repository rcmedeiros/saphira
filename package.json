{
  "name": "saphira",
  "version": "1.4.5-dev",
  "description": "Simple Opinionated API Framwwork for Microservices",
  "main": "index",
  "scripts": {
    "build:dev": "tsc -p tsconfig.json --removeComments false --listFiles false --sourceMap false --inlineSourceMap true --inlineSources true --listEmittedFiles true --declaration true && cp \"package.json\" \"dist/\"",
    "build:prod": "tsc -p tsconfig.json  --removeComments true --listFiles false --sourceMap false --inlineSourceMap false --inlineSources false --listEmittedFiles true --declaration true && cp \"package.json\" \"dist/\"",
    "audit": "snyk test && npm audit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "TS_NODE_PROJECT=test/tsconfig.json NODE_ENV=test nyc mocha --sort --trace-warnings --require ts-node/register --check-leaks --recursive --exit test/**/*test.ts",
    "coverage": " TS_NODE_PROJECT=test/tsconfig.json NODE_ENV=test nyc --reporter=html mocha --sort --require ts-node/register --check-leaks --recursive --exit test/**/*test.ts",
    "docs": "python -m mkdocs serve"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        75,
        90
      ],
      "statements": [
        80,
        95
      ]
    },
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "test/*"
    ],
    "cache": false,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rcmedeiros/saphira.git"
  },
  "keywords": [
    "framework",
    "typescript",
    "solid"
  ],
  "author": "Rafael Medeiros <rafael@rafaelmedeiros.com.br>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rcmedeiros/saphira/issues"
  },
  "homepage": "https://github.com/rcmedeiros/saphira#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.12",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/figlet": "^1.5.4",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/needle": "^2.5.3",
    "@types/node-rsa": "^1.1.1",
    "@types/pretty-hrtime": "^1.0.1",
    "@types/sinon": "^10.0.10",
    "@types/sshpk": "^1.17.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-rsa": "^1.1.1",
    "nyc": "^15.1.0",
    "openapi-types": "^10.0.0",
    "prettier": "^2.5.1",
    "selfsigned": "^2.0.0",
    "sinon": "^13.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@rcmedeiros/prototypes": "^1.4.0",
    "cert-info": "^1.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "fast-safe-stringify": "^2.1.1",
    "figlet": "^1.5.2",
    "helmet": "^5.0.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "logform": "^2.3.2",
    "needle": "^3.0.0",
    "pretty-hrtime": "^1.0.3",
    "sshpk": "^1.17.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.5.1",
    "winston-daily-rotate-file": "^4.6.0",
    "winston-transport": "^4.4.2"
  }
}
