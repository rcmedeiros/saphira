{
  "name": "saphira",
  "version": "0.0.2-dev",
  "description": "Simple Opinionated API Framwwork for Microservices",
  "main": "index",
  "scripts": {
    "build:dev": "tsc -p tsconfig.json --removeComments false --listFiles true --sourceMap false --inlineSourceMap true --inlineSources true --listEmittedFiles true --declaration true & cp \"package.json\" \"dist/\"",
    "build:prod": "tsc -p tsconfig.json  --removeComments true --listFiles true --sourceMap false --inlineSourceMap false --inlineSources false --listEmittedFiles true --declaration true & cp \"package.json\" \"dist/\"",
    "audit": "snyk test && npm audit",
    "lint": "echo FIXME",
    "test": "set TS_NODE_PROJECT=test/tsconfig.json&&set NODE_ENV=test&& nyc mocha --trace-warnings --require ts-node/register --check-leaks --recursive -exit test/**/*test.ts",
    "coverage": "set TS_NODE_PROJECT=test/tsconfig.json&&set NODE_ENV=test&& nyc --reporter=html mocha --require ts-node/register --check-leaks --recursive -exit test/**/*test.ts",
    "docs": "python -m mkdocs serve"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        75,
        90
      ],
      "statements": [
        80,
        95
      ]
    },
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "test/*"
    ],
    "cache": false,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rcmedeiros/saphira.git"
  },
  "keywords": [
    "framework",
    "typescript",
    "solid"
  ],
  "author": "Rafael Medeiros <rafael@rafaelmedeiros.com.br>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rcmedeiros/saphira/issues"
  },
  "homepage": "https://github.com/rcmedeiros/saphira#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/express-serve-static-core": "^4.17.14",
    "@types/figlet": "^1.2.1",
    "@types/helmet": "^4.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/logform": "^1.10.1",
    "@types/mocha": "^8.0.4",
    "@types/needle": "^2.5.1",
    "@types/pretty-hrtime": "^1.0.0",
    "@types/sshpk": "^1.10.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "selfsigned": "^1.10.8",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@rcmedeiros/prototypes": "^1.3.0-319",
    "body-parser": "^1.19.0",
    "cert-info": "^1.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "helmet": "^4.2.0",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "logform": "^2.2.0",
    "needle": "^2.5.2",
    "pretty-hrtime": "^1.0.3",
    "sshpk": "^1.16.1",
    "swagger-ui-express": "^4.1.5",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-transport": "^4.4.0"
  }
}
