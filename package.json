{
  "name": "saphira",
  "version": "0.0.1",
  "description": "",
  "main": "index",
  "scripts": {
    "dist": "tsc -p tsconfig.json & cp \"package.json\" \"dist/\"",
    "audit": "snyk test && npm audit",
    "tslint": "tslint --project tsconfig.json --config src/tslint.json",
    "test": "set TS_NODE_PROJECT=test/tsconfig.json&&set NODE_ENV=test&& nyc mocha --trace-warnings --require ts-node/register --check-leaks --recursive -exit test/**/*test.ts",
    "coverage": "set TS_NODE_PROJECT=test/tsconfig.json&&set NODE_ENV=test&& nyc --reporter=html mocha --require ts-node/register --check-leaks --recursive -exit test/**/*test.ts",
    "docs": "python -m mkdocs serve"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        75,
        90
      ],
      "statements": [
        80,
        95
      ]
    },
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "test/*"
    ],
    "cache": false,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcmedeiros/saphira.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rcmedeiros/saphira/issues"
  },
  "homepage": "https://github.com/rcmedeiros/saphira#readme",
  "devDependencies": {
    "@rcmedeiros/tslint-config": "^1.0.2",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/figlet": "^1.2.0",
    "@types/helmet": "0.0.43",
    "@types/logform": "^1.2.0",
    "@types/mocha": "^5.2.6",
    "@types/pretty-hrtime": "^1.0.0",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.22.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "selfsigned": "^1.10.4",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@rcmedeiros/prototypes": "^1.0.1-247",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "figlet": "^1.2.1",
    "helmet": "^3.18.0",
    "logform": "^2.1.2",
    "pretty-hrtime": "^1.0.3",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0",
    "winston-transport": "^4.3.0"
  }
}
